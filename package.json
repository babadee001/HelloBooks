{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Simple application that helps manage a library and its processes",
  "main": "server.js",
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov mocha --reporter spec --timeout 40000 --compilers js:babel-core/register test",
    "start": "nodemon --exec babel-node server.js",
    "lint": "./node_modules/.bin/eslint .",
    "test": "set NODE_ENV=test ./node_modules/.bin/mocha --timeout 40000 --compilers js:babel-core/register",
    "cover": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha  -- -R spec --timeout 40000"
  },
  "engines": {
    "node": "6.10.1"
  },
  "keywords": [
    "library"
  ],
  "author": "temidayo oyedele",
  "license": "ISC",
  "dependencies": {
