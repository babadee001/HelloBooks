    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.3.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "jsonwebtoken": "^7.4.2",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint-config-airbnb": "^15.1.0",
